#include <iostream>
using namespace std;

int main() {
    string str;
    int flag = 1;

    cout << "Enter a string: ";
    cin >> str;

    int n = str.length();

    for(int i = 0; i < n / 2; i++) {
        if(str[i] != str[n - i - 1]) {
            flag = 0;
            break;
        }
    }

    if(flag)
        cout << "Palindrome";
    else
        cout << "Not a palindrome";

    return 0;
}
